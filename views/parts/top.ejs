    <script>
      // Highlight current page in navbar.
      $(document).ready(function() {
        switch(window.location.pathname) {
          case "/login":
            $("#navbar a.login").addClass("currpage");
            break;
          case "/register":
            $("#navbar a.reg").addClass("currpage");
            break;
          case "/list":
            $("#navbar a.list").addClass("currpage");
            break;
          case "/create":
            $("#navbar a.create").addClass("currpage");
            break;
        }
      });
    </script>
